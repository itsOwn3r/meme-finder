{
    "name": "Meme Finder",
    "short_name": "Meme Finder",
    "theme_color": "#0c0d0e",
    "background_color": "#0c0d0e",
    "display": "standalone",
    "orientation": "portrait",
    "scope": "/",
    "start_url": "/",
    "share_target": {
        "action": "/api/pwa",
        "enctype": "multipart/form-data",
        "method": "POST",
        "params": {
          "title": "title",
          "text": "text",
          "url": "url",
          "files": [
            {
              "name": "file",
              "accept": [
                "image/jpeg",
                "image/png",
                "image/gif"
              ]
            }
          ]
        }
    },
    "icons": [
      {
          "src": "/icons/android-chrome-192x192.png",
          "sizes": "192x192",
          "type": "image/png"
      },
      {
          "src": "/icons/android-chrome-512x512.png",
          "sizes": "512x512",
          "type": "image/png"
      },
      {
        "src": "/icons/android-chrome-maskable-192x192.png",
        "sizes": "192x192",
        "type": "image/png",
        "purpose": "maskable"
      },
      {
        "src": "/icons/android-chrome-maskable-512x512.png",
        "sizes": "512x512",
        "type": "image/png",
        "purpose": "maskable"
      }
    ],
    "splash_pages": null
  }